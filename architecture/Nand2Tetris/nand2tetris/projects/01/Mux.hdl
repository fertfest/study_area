/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not (in=sel, out=nots);
    And (a=a, b=nots, out=aAndNots);
    And (a=sel, b=b, out=sAndb);
    Or (a=aAndNots, b=sAndb, out=out);
}
